{
  "Parameters": {
    "Name": {
      "Type": "String"
    }
  },
  "Resources": {
    "myVPC": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": "10.0.0.0/16",
        "EnableDnsHostnames": "true",
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "vpc"
                ]
              ]
            }
          }
        ]
      }
    },
    "mySubnet1": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        },
        "MapPublicIpOnLaunch": "true",
        "CidrBlock": "10.0.1.0/24",
        "AvailabilityZone": {
          "Fn::Select": [
            "0",
            {
              "Fn::GetAZs": "us-east-1"
            }
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "subnet1"
                ]
              ]
            }
          }
        ]
      }
    },
    "mySubnet2": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        },
        "MapPublicIpOnLaunch": "true",
        "CidrBlock": "10.0.2.0/24",
        "AvailabilityZone": {
          "Fn::Select": [
            "1",
            {
              "Fn::GetAZs": "us-east-1"
            }
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "subnet2"
                ]
              ]
            }
          }
        ]
      }
    },
    "mySubnet3": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        },
        "MapPublicIpOnLaunch": "true",
        "CidrBlock": "10.0.3.0/24",
        "AvailabilityZone": {
          "Fn::Select": [
            "2",
            {
              "Fn::GetAZs": "us-east-1"
            }
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "subnet3"
                ]
              ]
            }
          }
        ]
      }
    },
    "mySubnet4": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        },
        "CidrBlock": "10.0.4.0/24",
        "AvailabilityZone": {
          "Fn::Select": [
            "0",
            {
              "Fn::GetAZs": "us-east-1"
            }
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "subnet4"
                ]
              ]
            }
          }
        ]
      }
    },
    "mySubnet5": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        },
        "CidrBlock": "10.0.5.0/24",
        "AvailabilityZone": {
          "Fn::Select": [
            "1",
            {
              "Fn::GetAZs": "us-east-1"
            }
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "subnet5"
                ]
              ]
            }
          }
        ]
      }
    },
    "mySubnet6": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        },
        "CidrBlock": "10.0.6.0/24",
        "AvailabilityZone": {
          "Fn::Select": [
            "2",
            {
              "Fn::GetAZs": "us-east-1"
            }
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "subnet6"
                ]
              ]
            }
          }
        ]
      }
    },
    "myInternetGateway": {
      "Type": "AWS::EC2::InternetGateway",
      "Properties": {
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "InternetGateway"
                ]
              ]
            }
          }
        ]
      }
    },
    "myVPCGatewayAttachment": {
      "Type": "AWS::EC2::VPCGatewayAttachment",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        },
        "InternetGatewayId": {
          "Ref": "myInternetGateway"
        }
      }
    },
    "myRouteTable1": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "public-route-table"
                ]
              ]
            }
          }
        ]
      }
    },
    "myRouteTable2": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": {
          "Ref": "myVPC"
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "",
                [
                  {
                    "Ref": "Name"
                  },
                  "private-route-table"
                ]
              ]
            }
          }
        ]
      }
    },
    "mySubnetRouteTableAssociation1": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "mySubnet1"
        },
        "RouteTableId": {
          "Ref": "myRouteTable1"
        }
      }
    },
    "mySubnetRouteTableAssociation2": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "mySubnet2"
        },
        "RouteTableId": {
          "Ref": "myRouteTable1"
        }
      }
    },
    "mySubnetRouteTableAssociation3": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "mySubnet3"
        },
        "RouteTableId": {
          "Ref": "myRouteTable1"
        }
      }
    },
    "mySubnetRouteTableAssociation4": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "mySubnet4"
        },
        "RouteTableId": {
          "Ref": "myRouteTable2"
        }
      }
    },
    "mySubnetRouteTableAssociation5": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "mySubnet5"
        },
        "RouteTableId": {
          "Ref": "myRouteTable2"
        }
      }
    },
    "mySubnetRouteTableAssociation6": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "mySubnet6"
        },
        "RouteTableId": {
          "Ref": "myRouteTable2"
        }
      }
    },
    "myRoute": {
      "Type": "AWS::EC2::Route",
      "Properties": {
        "RouteTableId": {
          "Ref": "myRouteTable1"
        },
        "DestinationCidrBlock": "0.0.0.0/0",
        "GatewayId": {
          "Ref": "myInternetGateway"
        }
      }
    }
  },
  "Outputs": {
    "myVPCOutput": {
      "Value": {
        "Ref": "myVPC"
      },
      "Export": {
        "Name": "myVPC"
      }
    },
    "mySubnet1Output": {
      "Value": {
        "Ref": "mySubnet1"
      },
      "Export": {
        "Name": "mySubnet1"
      }
    },
    "mySubnetsOutput": {
      "Value": {
        "Fn::Join": [
          ",",
          [
            {
              "Ref": "mySubnet1"
            },
            {
              "Ref": "mySubnet2"
            },
            {
              "Ref": "mySubnet3"
            }
          ]
        ]
      },
      "Export": {
        "Name": "mySubnets"
      }
    },
    "myAvailabilityZone1Output": {
      "Value": {
        "Fn::GetAtt": [
          "mySubnet1",
          "AvailabilityZone"
        ]
      },
      "Export": {
        "Name": "myAvailabilityZone1"
      }
    }
  }
}
